{
  "name": "cloudisense-api-library",
  "version": "0.0.1",
  "description": "Api bridge for cloudisense client & service",
  "main": "lib/index.js",
  "module": "dist/index.esm.js",
  "types": "lib/index.d.ts",
  "author": "Connessione Technologies connessionetechnologies@gmail.com",
  "license": "Apache 2.0",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",    
    "prod": "tsc --sourceMap false && node ./lib/index.js",
    "debug": "tsc --sourcemap",
    "build_linux": "rm -rf ./dist && tsc --sourceMap false && tsc --build tsconfig.es5.json",
    "share_linux": "rm -rf ../cloudisense-client-react/src/dist  && cp -R ./dist ../cloudisense-client-react/src/dist",
    "compile_win": "rmdir /q /S .\\dist && tsc --sourceMap false && tsc --build tsconfig.es5.json",
    "share_win": "xcopy .\\dist\\ ..\\cloudisense-client-react\\src\\dist\\ /E /Y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connessionetech/cloudisense-client-lib.git"
  },
  "keywords": [],  
  "bugs": {
    "url": "https://github.com/connessionetech/cloudisense-api-connector/issues"
  },
  "homepage": "https://github.com/connessionetech/cloudisense-api-connector#readme",
  "devDependencies": {
    "@types/node": "^7.0.71",
    "prettier": "^2.2.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "defer-promise": "^2.0.1",
    "js-base64": "^3.6.1",
    "js-sha256": "^0.9.0",
    "json2typescript": "^1.4.1",
    "nanoid": "^3.1.23",
    "reflect-metadata": "^0.1.13",
    "strongly-typed-events": "^2.1.7",
    "tsify": "^5.0.2",
    "websocket": "^1.0.34"
  }
}
